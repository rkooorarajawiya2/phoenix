{
  "presets": [
    [
      "@babel/preset-env", {
        "debug": true,
        "useBuiltIns": "entry"
      }
    ]
  ],
  "plugins": ["@babel/plugin-syntax-dynamic-import",
    "transform-es2015-arrow-functions",
    "transform-es2015-shorthand-properties",
    "@babel/plugin-transform-classes"
  ]
}
